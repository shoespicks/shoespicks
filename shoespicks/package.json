    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "sass": "^1.32.13",
    "sass-loader": "^10.2.0",
