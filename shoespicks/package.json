    "@nuxtjs/markdownit": "^2.0.0",
    "core-js": "^3.12.1",
    "fibers": "^5.0.0",
    "firebase": "^8.6.1",
    "nuxt": "^2.15.6",
    "vuetify": "^2.5.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@nuxt/types": "^2.15.6",
